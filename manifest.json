{
  "manifest_version": 2,

  "name": "Cookie Blocker",
  "description": "This extension blocks sending any cookiee stored on your Chrome",
  "version": "1.0",
  "permissions": [
      "webRequest",
      "webRequestBlocking",
      "http://*/",
      "https://*/"
  ],
  "background": {
      "scripts" : ["js/background.js"],
      "persistent": false
  }
  "browser_action": {
	  "default_title": "Cookie Blocker"
  }
}
